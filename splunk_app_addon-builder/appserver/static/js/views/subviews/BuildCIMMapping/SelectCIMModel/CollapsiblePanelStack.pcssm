.CollapsiblePanel{
    width:100%;
    .content{
        .title{
            width:100%;
            .leadingButton{
                padding-right:9px;
            }
            button{
                padding-left:0px;
            }
        }
        .body{
            width: calc(100% - 40px);
            padding-left: 24px;
        }
    }
}

.hideButton{
    button {
        display:none;
    }
    padding-left:5px;
}

.iconStyle{
    width:9px;
    height:9px;
    margin-right:2%;
    color: #9b9b9b;
}
.pathContainer{
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.flexWraper {
    display: flex;
    align-items: center;
    height: 33px;
}
