.treeNode{
    position:relative;
    button{
        padding-left:0px;
    }
}
.leadingButton{
    padding-left:10px;
    cursor:pointer;
}
.titleLink{
    padding:5px 10px;
    cursor:pointer;
}
.linkCheckbox{
    right: 10px;
    top: 0px;
    cursor: pointer;
    margin: auto;
    position: absolute;
}
.emptyPadding{
    width: 1.25em;
    height: 0.4425em;
    vertical-align: middle;
    display: inline-block;
}
.content{
    margin-left: 15px;
}
.errorMessage{
    display:inline;
}
.disabledLink{
    cursor:no-drop;
    color:#5c6773;
}
