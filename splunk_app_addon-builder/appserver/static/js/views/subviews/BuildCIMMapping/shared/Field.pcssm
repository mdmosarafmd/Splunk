.root{
    position: relative;
    padding: 4px 0 4px 12px;
    height: 20px;
    line-height: 20px;

}

.rootEffect {
    composes: root;
    cursor: pointer;
}
.rootEffect:hover{
    background-color: rgba(0,0,0,.05);
}
.text{
    cursor: default;
    width: calc(100% - 16px);
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    user-select: none;
    display:inline-block;
    margin-right:16px;
}
.textEffect {
    composes: text;
    cursor: pointer;
}
.icon{
    position: absolute;
    font-size: 16px;
    color: #65a637;
    width: 12px;
    height: 12px;
    bottom: 0;
    margin: auto;
}

.leftIcon{
    composes: icon;
    left: 0;
    top: 0;
}
.rightIcon{
    composes: icon;
    right: 0;
    top: 0;
}
