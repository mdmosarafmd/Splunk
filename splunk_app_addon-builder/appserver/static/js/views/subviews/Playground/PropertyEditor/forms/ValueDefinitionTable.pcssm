.root{
    width:100%;
    height: 100%;
}
.table{
    tr{
        th{
            font-size: 10px;
            font-weight: 400;
            color: #3c444d;
            width: 120px;
            text-align: left;
        }
        th:nth-child(3){
            width: 70px;
        }
        td{
            width: 120px;
        }
        td:nth-child(3){
            width: 70px;
        }
    }
}
.remove{
    color: rgba(155, 155, 155, 0.6);
    cursor: pointer;
}
.remove:hover{
    color: #9b9b9b;
}
