.root {
    width: 100%;
    height: 100%;
}
.breadCrumbHead{
    width: 100%;
    height: 60px;
    box-sizing: border-box;
    padding:20px;
    background: #f5f5f5;
    font-family: Roboto;
    color: #3c444d;
}
.breadCrumbHeadWithDescription{
    composes: breadCrumbHead;
    height: 100px;
}
.breadCrumbHeadDescription {
    margin-top: 5px;
    font-family: Roboto;
    font-size: 12px;
    font-weight: 400;
    line-height: 14px;
    color: #4a4a4a;
}
.breadCrumbBody{
    width: 100%;
    height: calc(100% - 60px);
}
.breadCrumbBodyWithDescription{
    composes: breadCrumbBody;
    height: calc(100% - 100px);
}
.previousTitle{
    font-family: Roboto;
}
.title{
    font-family: Roboto;
    color: #3c444d;
    font-size: 18px;
    font-weight: bold;
}

.middelAlign{
    display:inline-block;
    vertical-align: middle;
}
